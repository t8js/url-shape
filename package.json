{
  "name": "url-shape",
  "version": "1.3.9",
  "description": "Type-safe schema-based URL builder",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "node -e \"require('node:fs').rmSync('dist', { force: true, recursive: true });\"",
    "compile": "npx esbuild index.ts --bundle --outdir=dist --platform=neutral --external:path-to-regexp",
    "preversion": "npx npm-run-all clean shape compile test",
    "shape": "npx codeshape --typecheck --emit-types",
    "test": "node tests.ts"
  },
  "author": "axtk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/t8js/url-shape.git"
  },
  "keywords": [
    "url builder",
    "url",
    "schema",
    "typed schema",
    "type-safe",
    "zod",
    "yup"
  ],
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "path-to-regexp": "^8.3.0",
    "quasiurl": "^1.1.8"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",
    "zod": "^4.1.13"
  }
}
